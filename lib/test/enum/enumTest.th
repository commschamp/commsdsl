void EnumTestSuite::test16()
{
    auto protocol = prepareProtocol(SCHEMAS_DIR "/Schema16.xml");
    TS_ASSERT(protocol);
    auto namespaces = protocol->parseLastParsedSchema().parseNamespaces();
    TS_ASSERT_EQUALS(namespaces.size(), 1U);

    auto& ns = namespaces.front();
    TS_ASSERT(ns.parseName().empty());

    auto fields = ns.parseFields();
    TS_ASSERT_EQUALS(fields.size(), 1U);

    auto& field = fields.front();
    TS_ASSERT_EQUALS(field.parseName(), "Enum1");
    TS_ASSERT(field.parseDisplayName().empty());
    TS_ASSERT(field.parseDescription().empty());
    TS_ASSERT_EQUALS(field.parseKind(), commsdsl::parse::ParseField::ParseKind::Enum);

    commsdsl::parse::ParseEnumField enumField(field);
    auto& values = enumField.parseValues();
    auto& revValues = enumField.parseRevValues();
    TS_ASSERT_EQUALS(enumField.parseType(), commsdsl::parse::ParseIntField::ParseType::Uint32);
    TS_ASSERT_EQUALS(enumField.parseEndian(), commsdsl::parse::ParseEndian_Little);
    TS_ASSERT_EQUALS(enumField.parseMinLength(), 3U);
    TS_ASSERT_EQUALS(enumField.parseDefaultValue(), 2);
    TS_ASSERT(enumField.parseIsNonUniqueAllowed());
    TS_ASSERT(!enumField.parseIsUnique());
    TS_ASSERT_EQUALS(values.size(), 3);
    TS_ASSERT_EQUALS(revValues.size(), values.size());
    auto& v1 = values.find("V1")->second;
    auto& v2 = values.find("V2")->second;
    auto& v3 = values.find("V3")->second;
    TS_ASSERT_EQUALS(v1.m_value, 1);
    TS_ASSERT_EQUALS(v1.m_displayName, "Value1");
    TS_ASSERT_EQUALS(v2.m_value, 2);
    TS_ASSERT_EQUALS(v2.m_displayName, "Value2");
    TS_ASSERT_EQUALS(v3.m_value, 2);
    TS_ASSERT_EQUALS(v3.m_displayName, "Value3");
    TS_ASSERT_EQUALS(revValues.find(1)->second, "V1");
    auto er = revValues.equal_range(2);
    TS_ASSERT_EQUALS(std::distance(er.first, er.second), 2);
}

