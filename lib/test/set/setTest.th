    TS_ASSERT_EQUALS(b0.m_reserved, false);
    TS_ASSERT_EQUALS(b0.m_reservedValue, false);
    TS_ASSERT_EQUALS(b2.m_idx, 2);
    TS_ASSERT_EQUALS(b8.m_idx, 8);
