void IntTestSuite::test43()
{
    std::vector<std::string> schemas = {
        SCHEMAS_DIR "/Schema43_1.xml",
        SCHEMAS_DIR "/Schema43_2.xml"
    };

    auto protocol = prepareProtocol(schemas, true);
    TS_ASSERT(protocol);

    auto namespaces = protocol->lastParsedSchema().namespaces();
    TS_ASSERT_EQUALS(namespaces.size(), 1U);

    auto& ns = namespaces.front();
    TS_ASSERT(ns.name().empty());    

    auto fields = ns.fields();
    do {
        TS_ASSERT_LESS_THAN_EQUALS(1U, fields.size());    

        auto& f1 = fields[0];
        TS_ASSERT_EQUALS(f1.kind(), commsdsl::parse::ParseField::Kind::Int);

        commsdsl::parse::ParseIntField intF1(f1);
        TS_ASSERT_EQUALS(intF1.defaultValue(), 1);
        auto& specials = intF1.specialValues();
        auto sp1Iter = specials.find("S1");
        TS_ASSERT(sp1Iter != specials.end());
        TS_ASSERT_EQUALS(sp1Iter->second.m_value, 1U);
    } while (false);

    do {
        TS_ASSERT_LESS_THAN_EQUALS(2U, fields.size());    

        auto& f2 = fields[1];
        TS_ASSERT_EQUALS(f2.kind(), commsdsl::parse::ParseField::Kind::Int);

        commsdsl::parse::ParseIntField intF2(f2);
        auto& specials = intF2.specialValues();
        auto sp1Iter = specials.find("S1");
        TS_ASSERT(sp1Iter != specials.end());
        TS_ASSERT_EQUALS(sp1Iter->second.m_value, 1U);
    } while (false);    
}