void OptionalTestSuite::test31()
{
    auto protocol = prepareProtocol(SCHEMAS_DIR "/Schema31.xml");
    TS_ASSERT(protocol);

    auto namespaces = protocol->namespaces();
    TS_ASSERT_EQUALS(namespaces.size(), 1U);

    auto& ns = namespaces.front();
    TS_ASSERT(ns.name().empty());

    auto fields = ns.fields();
    do {
        TS_ASSERT_LESS_THAN_EQUALS(1U, fields.size());

        auto& o1 = fields[0];
        TS_ASSERT_EQUALS(o1.kind(), commsdsl::parse::Field::Kind::Optional);

        commsdsl::parse::OptionalField opt(o1);
        TS_ASSERT(opt.missingOnReadFail());
        TS_ASSERT(!opt.missingOnInvalid());
    } while (false);

    do {
        TS_ASSERT_LESS_THAN_EQUALS(2U, fields.size());

        auto& o2 = fields[1];
        TS_ASSERT_EQUALS(o2.kind(), commsdsl::parse::Field::Kind::Optional);

        commsdsl::parse::OptionalField opt(o2);
        TS_ASSERT(!opt.missingOnReadFail());
        TS_ASSERT(opt.missingOnInvalid());
    } while (false);

}

